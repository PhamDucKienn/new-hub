-- Script dịch chuyển đến tất cả các rương trên bản đồ trong Blox Fruits
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Hàm tìm tất cả các rương trên bản đồ
local function findChests()
    local chests = {}
    for _, instance in pairs(workspace:GetDescendants()) do
        if instance.Name == "Chest" and instance:FindFirstChild("ClickDetector") then
            table.insert(chests, instance)
        end
    end
    return chests
end

-- Hàm dịch chuyển đến rương
local function teleportToChest(chest)
    humanoidRootPart.CFrame = chest.CFrame + Vector3.new(0, 5, 0) -- Di chuyển đến vị trí của rương
    wait(1) -- Đợi một chút để đảm bảo nhân vật đã đến vị trí
    fireclickdetector(chest.ClickDetector) -- Mở rương
end

-- Hàm chính để dịch chuyển đến tất cả các rương
local function teleportToAllChests()
    local chests = findChests()
    if #chests > 0 then
        for _, chest in pairs(chests) do
            teleportToChest(chest)
            wait(2) -- Đợi một chút trước khi di chuyển đến rương tiếp theo
        end
        print("Đã dịch chuyển đến tất cả các rương.")
    else
        print("Không tìm thấy rương nào trên bản đồ.")
    end
end

-- Gọi hàm chính
teleportToAllChests()
